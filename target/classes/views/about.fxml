<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" id="aboutPanel" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="113.0" prefWidth="280.0" stylesheets="/styles/style.css"
          xmlns="http://javafx.com/javafx/8.0.121" fx:controller="controllers.AboutController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="109.0" minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="84.0" minHeight="10.0" prefHeight="13.0" vgrow="SOMETIMES"/>
        <RowConstraints fillHeight="false" maxHeight="59.0" minHeight="10.0" prefHeight="59.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <VBox id="aboutInfosVBox" alignment="BOTTOM_CENTER" prefHeight="150.0" prefWidth="280.0">
            <children>
                <Label text="Auteur : Florian RENARD (France)">
                    <font>
                        <Font size="19.0"/>
                    </font>
                </Label>
                <Label text="Licence : GNU General Public License v3.0">
                    <font>
                        <Font size="19.0"/>
                    </font>
                </Label>
            </children>
        </VBox>
        <VBox id="aboutCloseButtonVBox" alignment="TOP_CENTER" prefHeight="52.0" prefWidth="280.0"
              GridPane.rowIndex="2">
            <children>
                <Button id="aboutCloseButton" alignment="TOP_CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                        onAction="#aboutCloseButtonClicked" prefWidth="65.0" text="OK"/>
            </children>
        </VBox>
    </children>
</GridPane>
